{
	"name": "@jjuidev/jss-devtools",
	"version": "0.0.1",
	"description": "Development tools CLI for Javascript stack like React, RN, NextJS, Nest, etc.",
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/esm/index.d.ts",
				"default": "./dist/esm/index.js"
			},
			"require": {
				"types": "./dist/cjs/index.d.ts",
				"default": "./dist/cjs/index.cjs"
			}
		}
	},
	"main": "./dist/cjs/index.cjs",
	"module": "./dist/esm/index.js",
	"bin": {
		"jss-devtools": "./dist/esm/index.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "bun run scripts/build.ts",
		"dev": "bun run --watch src/cli/index.ts",
		"changeset": "changeset",
		"clean": "rimraf dist",
		"release": "bun run clean && bun run build && changeset publish",
		"version": "changeset version"
	},
	"types": "./dist/esm/index.d.ts",
	"dependencies": {
		"@changesets/cli": "^2.29.8",
		"@clack/prompts": "^1.0.1",
		"ansis": "^4.2.0",
		"citty": "^0.2.1",
		"consola": "^3.4.2",
		"defu": "^6.1.4",
		"execa": "^9.6.1",
		"figlet": "^1.10.0",
		"globby": "^16.1.0",
		"nypm": "^0.6.5",
		"package-manager-detector": "^1.6.0",
		"pathe": "^2.0.3"
	},
	"peerDependencies": {
		"typescript": "^5.0.0"
	},
	"devDependencies": {
		"@types/bun": "^1.3.9",
		"@types/figlet": "^1.7.0",
		"@types/node": "^25.2.3",
		"rimraf": "^6.1.2",
		"typescript": "^5.9.3"
	},
	"keywords": [
		"commitlint",
		"devtools",
		"eslint",
		"husky",
		"lint-staged",
		"prettier"
	],
	"engines": {
		"node": ">=18"
	}
}

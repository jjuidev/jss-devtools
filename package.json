{
	"name": "@jjuidev/jss-devtools",
	"version": "0.0.3",
	"description": "Development tools CLI for Javascript stack like React, RN, NextJS, Nest, etc.",
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/esm/index.d.ts",
				"default": "./dist/esm/index.js"
			},
			"require": {
				"types": "./dist/cjs/index.d.ts",
				"default": "./dist/cjs/index.cjs"
			}
		}
	},
	"main": "./dist/cjs/index.cjs",
	"module": "./dist/esm/index.js",
	"bin": {
		"jss-devtools": "./dist/esm/index.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "bun run scripts/build.ts",
		"dev": "bun run --watch src/cli/index.ts",
		"changeset": "changeset",
		"clean": "rimraf dist",
		"release": "bun run clean && bun run build && changeset publish",
		"version": "changeset version",
		"prepare": "husky",
		"format": "eslint --fix \"src/**/*.{js,ts,jsx,tsx}\" && prettier --write \"src/**/*.{js,ts,jsx,tsx}\""
	},
	"types": "./dist/esm/index.d.ts",
	"dependencies": {
		"@clack/prompts": "^1.0.1",
		"citty": "^0.2.1",
		"consola": "^3.4.2",
		"defu": "^6.1.4",
		"execa": "^9.6.1",
		"figlet": "^1.10.0",
		"globby": "^16.1.0",
		"nypm": "^0.6.5",
		"package-manager-detector": "^1.6.0",
		"pathe": "^2.0.3"
	},
	"peerDependencies": {
		"typescript": "^5.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.8",
		"@commitlint/cli": "^20.4.2",
		"@commitlint/config-conventional": "^20.4.2",
		"@eslint/js": "^10.0.1",
		"@types/bun": "^1.3.9",
		"@types/figlet": "^1.7.0",
		"@types/node": "^25.2.3",
		"eslint": "^10.0.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-autofix": "^2.2.0",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-prefer-arrow-functions": "^3.9.1",
		"globals": "^17.3.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"prettier": "^3.8.1",
		"rimraf": "^6.1.2",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.0"
	},
	"keywords": [
		"commitlint",
		"devtools",
		"eslint",
		"husky",
		"lint-staged",
		"prettier"
	],
	"engines": {
		"node": ">=18"
	},
	"lint-staged": {
		"src/**/*.{js,ts,jsx,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"package.json": [
			"bunx prettier-package-json --write"
		]
	}
}
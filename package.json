{
	"name": "@jjuidev/jss-devtools",
	"version": "0.3.0",
	"description": "Development tools CLI for Javascript stack like React, RN, NextJS, Nest, etc.",
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/types/index.d.ts",
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.cjs"
		}
	},
	"main": "./dist/cjs/index.cjs",
	"module": "./dist/esm/index.js",
	"types": "./dist/types/index.d.ts",
	"bin": {
		"jss-devtools": "./dist/cli/cli.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "bun run src/build.ts",
		"changeset": "changeset",
		"clean": "rimraf dist",
		"dev": "bun run --watch src/cli/index.ts",
		"format": "eslint --fix \"src/**/*.{js,ts,jsx,tsx}\" && prettier --write \"src/**/*.{js,ts,jsx,tsx}\"",
		"release": "bun run clean && bun run build && changeset publish",
		"version": "changeset version"
	},
	"dependencies": {
		"@clack/prompts": "^1.0.1",
		"citty": "^0.2.1",
		"consola": "^3.4.2",
		"defu": "^6.1.4",
		"execa": "^9.6.1",
		"figlet": "^1.10.0",
		"globby": "^16.1.0",
		"nypm": "^0.6.5",
		"pathe": "^2.0.3"
	},
	"peerDependencies": {
		"@commitlint/cli": "^20.4.2",
		"@commitlint/config-conventional": "^20.4.2",
		"@eslint/js": "^9.0.0",
		"@next/eslint-plugin-next": "^16.0.1",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-autofix": "^2.2.0",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-prefer-arrow-functions": "^3.9.1",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-native": "^5.0.0",
		"eslint-plugin-storybook": "^10.2.11",
		"eslint-plugin-tailwindcss": "^3.18.2",
		"globals": "^17.3.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"prettier": "^3.8.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.1"
	},
	"peerDependenciesMeta": {
		"@next/eslint-plugin-next": {
			"optional": true
		},
		"eslint-plugin-react": {
			"optional": true
		},
		"eslint-plugin-react-hooks": {
			"optional": true
		},
		"eslint-plugin-react-native": {
			"optional": true
		},
		"eslint-plugin-storybook": {
			"optional": true
		},
		"eslint-plugin-tailwindcss": {
			"optional": true
		},
		"prettier-plugin-tailwindcss": {
			"optional": true
		}
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.8",
		"@types/bun": "^1.3.9",
		"@types/figlet": "^1.7.0",
		"@types/node": "^25.2.3",
		"rimraf": "^6.1.2",
		"tsc-alias": "^1.8.16",
		"typescript": "^5.9.3"
	},
	"keywords": [
		"commitlint",
		"devtools",
		"eslint",
		"husky",
		"lint-staged",
		"prettier"
	],
	"engines": {
		"node": ">=18"
	}
}
